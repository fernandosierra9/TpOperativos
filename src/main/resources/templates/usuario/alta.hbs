{{#partial "contenido"}}
    <h1>Dispositivos</h1>

    <table class="table table-stripped">
        <tr>
            <th>Nombre</th>
            <th>Equipo Concreto</th>
            <th>Consumo estimado por hora</th>
        </tr>

        {{#each dispositivos}}
            <tr>
                <td>{{nombre}}</td>
                <td>{{equipoConcreto}}</td>
                <td>{{consumoEstimadoPorHora}}</td>
                <td><a href="usuario/dispositivo/alta">Alta</a><td>

            </tr>
        {{/each}}
    </table>
{{/partial}}

{{>layout.hbs}}
